{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "clear": "php artisan cache:clear && php artisan config:clear && php artisan route:clear && php artisan view:clear && php artisan optimize:clear && php artisan config:cache &&php artisan route:cache && php artisan view:cache",
        "optimize": "composer dump-autoload && php artisan optimize",
        "migrate": "php artisan migrate:fresh",
        "seed:all": "php artisan db:seed",
        "seed:user": "php artisan db:seed --class=UserSeeder",
        "seed:category": "php artisan db:seed --class=CategorySeeder",
        "seed:post": "php artisan db:seed --class=PostSeeder",
        "seed:profile": "php artisan db:seed --class=ProfileSeeder"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.1.13",
        "typescript": "^5.9.2",
        "vite": "^7.0.4"
    }
}
